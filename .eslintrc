{
  "env": {
    "node": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules":{
    "no-cond-assign":2,
    "no-dupe-args":2,
    "no-dupe-keys":2,
    "no-duplicate-case":2,
    "no-empty":[2,{ "allowEmptyCatch": true }],
    "no-ex-assign":2,
    "no-extra-boolean-cast":2,
    "no-extra-parens":2,
    "no-extra-semi":1,
    "no-func-assign":2,
    "no-invalid-regexp":2,
    "no-sparse-arrays":2,
    "no-unreachable":2,
    "use-isnan":2,
    "curly":2,
    "default-case":2,
    "array-callback-return":2,
    "eqeqeq":2,
    "no-empty-function":2,
    "no-redeclare":2,
    "no-self-assign":2,
    "no-useless-return":1,
    "brace-style":[1,"1tbs"],
    "one-var":[2,"never"]
  }
}
